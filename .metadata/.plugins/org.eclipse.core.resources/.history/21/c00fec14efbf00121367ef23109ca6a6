import bb.cascades 1.0

StandardListItem{
    id: noteItem
    title: ListItemData.title
    imageSpaceReserved: false
    contextActions: [
        ActionSet{
        MultiSelectActionItem {
        multiSelectHandler: myList.multiSelectHandler
        onTriggered: {
        	multiSelectHandler.active = true
        	}
        }
    }

    ]
}