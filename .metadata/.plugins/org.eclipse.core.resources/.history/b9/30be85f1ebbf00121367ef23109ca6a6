import bb.cascades 1.0

StandardListItem{
    id: noteItem
    title: ListItemData.title
    imageSpaceReserved: false
    contextActions: [
        ActionSet{
            MultiSelectActionItem {
                multiSelectHandler: noteItem.ListItem.view.multiSelectHandler
                onTriggered: {
                    multiSelectHandler.active;
                }
            }
        }
        
    ]

}